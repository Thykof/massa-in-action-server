{
  "name": "massa-in-action-server",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon --watch src -e js,ts,json --exec \"ts-node src/index.ts\"",
    "build": "tsup src/index.ts --format cjs --clean",
    "start": "node dist/index.js",
    "prettier": "prettier --check src",
    "prettier:fix": "prettier --write src",
    "lint": "eslint src/**",
    "lint:fix": "npm run lint -- --fix",
    "fmt": "npm run prettier:fix && npm run lint:fix",
    "fmt:check": "npm run prettier && npm run lint"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@massalabs/sc-project-initializer": "^0.1.3",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "esbuild": "^0.17.5",
    "express": "^4.18.2",
    "npm": "^9.4.0"
  },
  "devDependencies": {
    "@massalabs/eslint-config": "^0.0.9",
    "@massalabs/prettier-config-as": "^0.0.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.16",
    "@types/node": "^18.11.18",
    "nodemon": "^2.0.20",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2",
    "tsup": "^6.5.0",
    "typescript": "^4.9.5"
  }
